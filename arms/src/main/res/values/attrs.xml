    <declare-styleable name="FunGameHitBlockHeader">
        <attr name="fghBlockHorizontalNum" format="integer" />
        <attr name="fghBallSpeed" format="dimension">
            <!--<enum name="low" value="1.5dp" />-->
            <!--<enum name="medium" value="3dp" />-->
            <!--<enum name="fast" value="4.5dp" />-->
        </attr>
