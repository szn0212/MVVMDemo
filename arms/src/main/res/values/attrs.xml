    <declare-styleable name="MountainSceneView">
        <attr name="msvPrimaryColor" format="color" />
        <attr name="msvViewportHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="FunGameView">

        <attr name="fghMaskTextTop" format="string" />
        <attr name="fghMaskTextTopPull" format="string" />
        <attr name="fghMaskTextTopRelease" format="string" />
        <attr name="fghMaskTextBottom" format="string" />
        <attr name="fghMaskTextSizeTop" format="dimension" />
        <attr name="fghMaskTextSizeBottom" format="dimension" />

        <attr name="fghBackColor" format="color" />
        <attr name="fghLeftColor" format="color" />
        <attr name="fghMiddleColor" format="color" />
        <attr name="fghRightColor" format="color" />
        <attr name="fghTextLoading" format="string" />
